#hoverpoint1 {
	position: absolute;
	top: 30vh;
	left: 15vw;
	opacity: .4;
	z-index: 1;
}

#hoverpoint1 #hoverpointtext1 {
	visibility: hidden;
	width: 120px;
	background-color: black;
	color: white;
	text-align: center;
	border-radius: 6px;
	padding: 5px 0;
	z-index: 1;
	bottom: 150%;
	left: 50%;
}

